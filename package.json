    "dev:frontend": "next dev",
    "build:frontend": "next build",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "download-fonts": "node scripts/download-font.js",
    "postinstall": "npm run download-fonts",
    "lint": "eslint",
    "test": "npm run patch-esm && jest",
    "test:watch": "npm run patch-esm && jest --watchAll",
    "test:coverage": "npm run patch-esm && jest --coverage",
    "test:ci": "npm run patch-esm && jest --ci --runInBand",
    "detect-esm": "node scripts/find-esm-deps.js",
    "patch-esm": "node scripts/patch-jest-esm.js",
    "dev:backend": "npm run dev --prefix ../Gicoin/backend",
    "dev:all": "npx concurrently \"npm run dev --prefix ../Gicoin/backend\" \"npm run dev:frontend\""
  },
  
